{
  "name": "atelier",
  "license": "MIT",
  "main": "expo-router/entry",
  "version": "0.1.1",
  "scripts": {
    "start": "yarn fix:version && expo start",
    "android": "yarn fix:version && expo run:android",
    "ios": "yarn fix:version && expo run:ios",
    "web": "yarn fix:version && expo start --web",
    "prebuild": "yarn fix:version && expo prebuild",
    "precommit": "yarn fix:version && yarn fix:prettier && yarn lint && yarn test",
    "fix:version": "tsx scripts/fix-version.ts",
    "fix:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "fix": "yarn fix:version && yarn fix:prettier",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/literata": "^0.2.3",
    "@react-navigation/native": "7.0.0-rc.20",
    "@tanstack/react-query": "^5.59.16",
    "expo": "~52.0.0-preview.5",
    "expo-blur": "~14.0.1",
    "expo-constants": "~17.0.1",
    "expo-font": "~13.0.0",
    "expo-haptics": "~14.0.0",
    "expo-linking": "~7.0.2",
    "expo-router": "~4.0.0-preview.3",
    "expo-splash-screen": "~0.28.2",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.1",
    "expo-web-browser": "~14.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-mmkv": "^3.1.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.11.0",
    "react-native-screens": "4.0.0-beta.14",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.0-preview.1",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.3.1",
    "tsx": "^4.19.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
